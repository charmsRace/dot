  /**
   * # Folding Chord System
   *
   * ## Keybindings
   *
   * > [⎈-⎇] numpad0 → ...
   *
   * ### Symmetric Fold/Unfold
   *
   * ... [⎈-⎇] {← →} :: un/fold at cursor
   * ... [⎈⇧⎇] {← →} :: un/fold recursively
   * ... [⎈--] {← →} :: un/fold all
   * ... [⎈⇧-] {← →} :: un/fold all except
   *
   * {←} ↦ fold
   * {→} ↦ unfold
   *
   * ### Other
   *
   * ... [⎈-⎇] ↓         :: toggle un/fold
   * ... [⎈⇧⎇] ↓         :: toggle un/fold recursively
   * ... [⎈-⎇] z         :: fold all block comments
   * ... [⎈-⎇] numpad{N} :: fold level N (N ∈ [7])
   *
   * ## Notes
   *
   * - ??? `editor.un/foldAllMarkerRegions`
   * - ??? `editor.createFoldingRangeFromSelection`
   */

  /* Un/fold at cursor */
  {
    "key": "ctrl+alt+numpad0 ctrl+alt+left",
    "command": "editor.fold",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+alt+numpad0 ctrl+alt+right",
    "command": "editor.unfold",
    "when": "editorTextFocus && foldingEnabled"
  },

  /* Un/fold recursively */
  {
    "key": "ctrl+alt+numpad0 ctrl+shift+alt+left",
    "command": "editor.foldRecursively",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+alt+numpad0 ctrl+shift+alt+right",
    "command": "editor.unfoldRecursively",
    "when": "editorTextFocus && foldingEnabled"
  },

  /* Un/fold all */
  {
    "key": "ctrl+alt+numpad0 ctrl+left",
    "command": "editor.foldAll",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+alt+numpad0 ctrl+right",
    "command": "editor.unfoldAll",
    "when": "editorTextFocus && foldingEnabled"
  },

  /* Un/fold all except */
  {
    "key": "ctrl+alt+numpad0 ctrl+shift+left",
    "command": "editor.foldAllExcept",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+alt+numpad0 ctrl+shift+right",
    "command": "editor.unfoldAllExcept",
    "when": "editorTextFocus && foldingEnabled"
  },

  /* Toggle un/fold */
  {
    "key": "ctrl+alt+numpad0 ctrl+alt+down",
    "command": "editor.toggleFold",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+alt+numpad0 ctrl+shift+alt+down",
    "command": "editor.toggleFoldRecursively",
    "when": "editorTextFocus && foldingEnabled"
  },

  /* Fold all block comments */
  {
    "key": "ctrl+alt+numpad0 ctrl+alt+z",
    "command": "editor.foldAllBlockComments",
    "when": "editorTextFocus && foldingEnabled"
  },

  /* Fold level N */
  {
    "key": "ctrl+alt+numpad0 ctrl+alt+numpad1",
    "command": "editor.foldLevel1",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+alt+numpad0 ctrl+alt+numpad2",
    "command": "editor.foldLevel2",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
